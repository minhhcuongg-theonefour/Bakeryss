@model AnhThuBakery.Models.QUANTRI
@{
    ViewBag.Title = "dangnhapadmin";
}

<div class="panel panel-primary" style="width: 300px; margin-top: 20px;">
    <div class="panel-heading">
        <h3 class="panel-title">Đăng nhập quản trị</h3>
    </div>
    <div class="panel-body">
        @using (Html.BeginForm("Dangnhapadmin", "Admincp", FormMethod.Post))
        {

            <div class="form-group">
                <label>Tên đăng nhập</label>
                @Html.TextBoxFor(i => i.TenDN, new { @class = "form-control", placeholder = "Tên đăng nhập" })@Html.ValidationMessageFor(i => i.TenDN)
            </div>
            <div class="form-group">
                <label>Mật Khẩu</label>
                @Html.PasswordFor(i => i.MatKhau, new { @class = "form-control", placeholder = "Mật Khẩu" })@Html.ValidationMessageFor(i => i.TenDN)
            </div>
            <button type="submit" class="btn btn-success" style="float:right">Submit</button>
        }
    </div>
</div>